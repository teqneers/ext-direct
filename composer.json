{
    "name": "teqneers/ext-direct",
    "type": "library",
    "description": "A base component to integrate Sencha Ext JS Ext.direct into a PHP application",
    "homepage": "https://github.com/teqneers/ext-direct",
    "license": "MIT",
    "authors": [
        {
            "name": "Stefan Gehrig",
            "email": "gehrig@teqneers.de",
            "homepage": "https://www.teqneers.de/"
        },
        {
            "name": "Erik Theoboldt",
            "email": "theoboldt@teqneers.de"
        }
    ],
    "autoload": {
        "psr-4": {
            "TQ\\ExtDirect\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TQ\\ExtDirect\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.2.0",
        "ext-json": "*",
        "doctrine/annotations": "~1.0",
        "jms/metadata": "~2.0",
        "jms/serializer": "~2.0 || ~3.0",
        "symfony/dependency-injection": "~3.0 || ~4.0 || ~5.0",
        "symfony/event-dispatcher": "~3.0 || ~4.0 || ~5.0",
        "symfony/expression-language": "~3.0 || ~4.0 || ~5.0",
        "symfony/http-foundation": "~3.0 || ~4.0 || ~5.0",
        "symfony/security-core": "~3.0 || ~4.0 || ~5.0",
        "symfony/validator": "~3.0 || ~4.0 || ~5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "symfony/phpunit-bridge": "~5.0",
        "symfony/stopwatch": "~3.0 || ~4.0 || ~5.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0.x-dev"
        }
    }
}
